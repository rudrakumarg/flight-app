{
  "name": "luggauge-domain",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/luggauge/domain/src",
  "prefix": "luggauge",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/luggauge/domain/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/luggauge/domain/**/*.ts",
          "libs/luggauge/domain/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:luggauge", "type:domain-logic"]
}
